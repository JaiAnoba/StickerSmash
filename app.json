{
  "expo": {
    "name": "Burgify",
    "slug": "burgify",
    "jsEngine": "jsc",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/2.png",
      "resizeMode": "contain",
      "backgroundColor": "#8B0000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/2.png",
        "backgroundColor": "#8B0000"
      },
      "package": "com.anonymous.burgify"
    },
    "web": {
      "bundler": "metro"
    }
  }
}

